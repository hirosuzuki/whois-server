{
    "name": "dev golang whois server",
    // https://hub.docker.com/_/microsoft-devcontainers
    "image": "mcr.microsoft.com/devcontainers/go:1.20-bullseye",
    // https://containers.dev/features.
    "features": {
        "ghcr.io/devcontainers/features/docker-in-docker": "",
        "ghcr.io/dhoeric/features/google-cloud-cli": ""
    },
    "forwardPorts": [
        8080
    ],
    "portsAttributes": {
        "8080": {
            "protocol": "http"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "mhutchie.git-graph"
            ]
        }
    }
}